[user]
    name = Jamey J. DeOrio
    email = jameyd@darkhorse.com
[color]
    ui = true
[alias]
    st = status
    co = checkout
    l = log --graph --pretty=format:'%C(yellow)%h%C(cyan)%d%Creset %s %C(white)- %an, %ar%Creset'
    ci = commit

[hub]
protocol = https

[credential]
helper = /opt/boxen/bin/boxen-git-credential

[core]
excludesfile = /opt/boxen/config/git/gitignore
